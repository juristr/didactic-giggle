{
  "version": 2,
  "projects": {
    "imgs": {
      "root": "libs/imgs",
      "sourceRoot": "libs/imgs/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/imgs/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/imgs"],
          "options": {
            "jestConfig": "libs/imgs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "imgs-react-lib": {
      "root": "libs/imgs-react-lib",
      "sourceRoot": "libs/imgs-react-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/imgs-react-lib/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/imgs-react-lib"],
          "options": {
            "jestConfig": "libs/imgs-react-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "next-app": {
      "root": "apps/next-app",
      "sourceRoot": "apps/next-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/next-app",
            "outputPath": "dist/apps/next-app"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "next-app:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "next-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "next-app:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/next-app"],
          "options": {
            "jestConfig": "apps/next-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/next-app/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "next-app-e2e": {
      "root": "apps/next-app-e2e",
      "sourceRoot": "apps/next-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/next-app-e2e/cypress.json",
            "tsConfig": "apps/next-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "next-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "next-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/next-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "reactapp": {
      "root": "apps/reactapp",
      "sourceRoot": "apps/reactapp/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/reactapp",
            "index": "apps/reactapp/src/index.html",
            "main": "apps/reactapp/src/main.tsx",
            "polyfills": "apps/reactapp/src/polyfills.ts",
            "tsConfig": "apps/reactapp/tsconfig.app.json",
            "assets": [
              "apps/reactapp/src/favicon.ico",
              "apps/reactapp/src/assets"
            ],
            "styles": ["apps/reactapp/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/reactapp/src/environments/environment.ts",
                  "with": "apps/reactapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "reactapp:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "reactapp:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/reactapp/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/reactapp"],
          "options": {
            "jestConfig": "apps/reactapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "reactapp-e2e": {
      "root": "apps/reactapp-e2e",
      "sourceRoot": "apps/reactapp-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/reactapp-e2e/cypress.json",
            "tsConfig": "apps/reactapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "reactapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "reactapp:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/reactapp-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "next-app"
}
